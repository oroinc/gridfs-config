{
    "name": "oro/gridfs-config",
    "description": "GridFS adapter configuration for Gaufrette",
    "type": "symfony-bundle",
    "keywords": ["Oro", "OroPlatform", "Mongo", "MongoDB", "GridFS"],
    "homepage": "https://github.com/oroinc/gridfs-config",
    "license": "MIT",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "https://oroinc.com/"
        }
    ],
    "autoload": {
        "psr-4": {"Oro\\Bundle\\GridFSConfigBundle\\": "src/Oro/Bundle/GridFSConfigBundle"},
        "exclude-from-classmap": ["**/Tests/"]
    },
    "require": {
        "oro/platform": "7.0.*",
        "mongodb/mongodb": "^1.15 || ^2.0",
        "knplabs/knp-gaufrette-bundle": "~0.9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.13"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        }
    }
}
